{
    "description": {
        "text": [
            "Selector: .uiAutocompleteOption.",
            "Represents the uiAutocompleteOption component in the autocomplete combobox.",
            "Get an option label or select an option from dropdown list of selectable options."
        ],
        "author": "Salesforce"
    },
    "root": true,
    "selector": {
        "css": ".uiAutocompleteOption"
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "isPresent"
                        },
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "a div.uiBlock"
                                    }
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "methods": [
        {
            "name": "select",
            "compose": [
                {
                    "element": "optionLink",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "getOptionLabel",
            "compose": [
                {
                    "element": "optionLink",
                    "apply": "getTitle"
                }
            ]
        }
    ],
    "elements": [
        {
            "type": [
                "clickable"
            ],
            "public": true,
            "name": "optionLink",
            "selector": {
                "css": "a div.uiBlock"
            }
        }
    ]
}
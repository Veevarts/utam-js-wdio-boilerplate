{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: lst-dynamic-related-list-desktop.",
            "Represents the dynamic related list desktop app builder Lightning web component.",
            "Access the related list view manager."
        ]
    },
    "selector": {
        "css": "lst-dynamic-related-list-desktop"
    },

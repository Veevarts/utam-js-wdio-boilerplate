{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: lst-related-list-quick-links-container.",
            "Represents the related lists quick links container Lightning web component.",
            "Access the quick links grid."
        ]
    },
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "isPresent"
                        },
                        {
                            "element": "root",
                            "apply": "containsElement",
                            "args": [
                                {
                                    "type": "locator",
                                    "value": {
                                        "css": "lst-related-list-quick-links-grid"
                                    }
                                },
                                {
                                    "value": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ],
    "shadow": {
        "elements": [
            {
                "type": "utam-lists/pageObjects/relatedListQuickLinksGrid",
                "name": "quickLinksGrid",
                "public": true,
                "selector": {
                    "css": "lst-related-list-quick-links-grid"
                }
            }
        ]
    }
}
{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: lightning-vertical-navigation-overflow.",
            "Represents the lightning-vertical-navigation-overflow Lightning web component.",
            "Get the item count. Check if it's expanded or toggle the section.",
            "Get all the vertical navigation items, get the active item, an item by name or an item by index."
        ]
    },
    "methods": [
        {
            "name": "toggle",
            "description": "Click the toggle button",
            "compose": [
                {
                    "element": "toggleButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "itemCount",
            "description": {
                "text": [
                    "Get all navigation items count"
                ],
                "return": "number"
            },
            "compose": [
                {
                    "element": "items",
                    "apply": "size"
                }
            ]
        },
        {
            "name": "isExpanded",
            "description": {
                "text": [
                    "Get the aria-expanded attribute of toogle button and to check if it is expanded"
                ],
                "return": "string"
            },
            "compose": [
                {
                    "element": "toggleButton",
                    "apply": "getAttribute",
                    "args": [
                        {
                            "value": "aria-expanded"
                        }
                    ]
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "toggleButton",
            "type": [
                "clickable",
                "actionable"
            ],
            "selector": {
                "css": "button[class*='slds-button_reset']"
            }
        },
        {
            "name": "verticalNavigationItems",
            "type": "utam-force/pageObjects/verticalNavigationItem",
            "selector": {
                "css": "lightning-vertical-navigation-item",
                "returnAll": true
            },
            "public": true
        },
        {
            "name": "verticalNavigationItemIcons",
            "type": "utam-force/pageObjects/verticalNavigationItemIcon",
            "selector": {
                "css": "lightning-vertical-navigation-item-icon",
                "returnAll": true
            },
            "public": true
        },
        {
            "name": "verticalNavigationItemBadges",
            "type": "utam-force/pageObjects/verticalNavigationItemBadge",
            "selector": {
                "css": "lightning-vertical-navigation-item-badge",
                "returnAll": true
            },
            "public": true
        },
        {
            "name": "itemByName",
            "type": "utam-force/pageObjects/verticalNavigationItem",
            "public": true,
            "selector": {
                "css": ".slds-nav-vertical__item",
                "returnAll": true
            },
            "filter": {
                "apply": "getName",
                "findFirst": true,
                "matcher": {
                    "type": "stringContains",
                    "args": [
                        {
                            "name": "itemText",
                            "type": "string"
                        }
                    ]
                }
            }
        },
        {
            "name": "itemAtIndex",
            "type": "utam-force/pageObjects/verticalNavigationItem",
            "selector": {
                "css": ".slds-nav-vertical__item:nth-of-type(%d)",
                "args": [
                    {
                        "name": "index",
                        "type": "number"
                    }
                ]
            },
            "public": true
        },
        {
            "name": "items",
            "type": "utam-force/pageObjects/verticalNavigationItem",
            "selector": {
                "css": ".slds-nav-vertical__item",
                "returnAll": true
            },
            "public": true
        }
    ]
}
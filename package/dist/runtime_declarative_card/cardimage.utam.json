    },
    "shadow": {
        "elements": [
            {
                "name": "sldsImage",
                "type": [],
                "public": true,
                "selector": {
                    "css": ".slds-image"
                },
                "elements": [
                    {
                        "name": "imageTag",
                        "type": [],
                        "public": true,
                        "selector": {
                            "css": "img"
                        }
                    }
                ]
            },
            {
                "name": "altText",
                "type": [],

{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: **/XCUIElementTypeAlert.",
            "Represents the iOS alert view.",
            "Choose an action or get a message from the alert."
        ]
    },
    "implements": "utam-playgroundapp/pageObjects/dialog/alert",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "ios_phone"
            ]
        }
    ],
    "selector": {
        "classchain": "**/XCUIElementTypeAlert"
    },
    "methods": [
        {
            "name": "clickButton",
            "compose": [
                {
                    "element": "button",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "hasButton",
            "compose": [
                {
                    "returnType": "boolean",
                    "element": "button",
                    "apply": "isPresent"
                }
            ]
        },
        {
            "name": "getMessages",
            "compose": [
                {
                    "element": "messages",
                    "apply": "getText"
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "button",
            "type": [
                "clickable"
            ],
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`name == '%s'`]",
                "args": [
                    {
                        "name": "label",
                        "type": "string"
                    }
                ]
            }
        },
        {
            "name": "messages",
            "selector": {
                "classchain": "**/XCUIElementTypeStaticText",
                "returnAll": true
            },
            "type": [
                "actionable"
            ]
        }
    ]
}
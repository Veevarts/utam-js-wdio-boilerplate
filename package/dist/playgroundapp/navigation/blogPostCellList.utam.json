{
    "description": {
        "author": "Salesforce",
        "text": [
            "Represents the list of blog posts.",
            "Check if a blog post exists in the list of blogs."
        ]
    },
    "methods": [
        {
            "name": "hasBlog",
            "compose": [
                {
                    "returnType": "boolean",
                    "element": "blogListContainer",
                    "apply": "containsElement",
                    "args": [
                        {
                            "type": "locator",
                            "value": {
                                "css": "p.js-content-title.layout_title.slds-text-heading_medium[title='%s']",
                                "args": [
                                    {
                                        "name": "title",
                                        "type": "string"
                                    }
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "blogPostCells",
            "type": "utam-playgroundapp/pageObjects/navigation/blogPostCell",
            "public": true,
            "selector": {
                "css": "div.slds-large-size_1-of-3.slds-medium-size_1-of-2.slds-size_1-of-1.slds-p-right_small.slds-m-bottom_small",
                "returnAll": true
            }
        },
        {
            "name": "blogPostCellWithIndex",
            "type": "utam-playgroundapp/pageObjects/navigation/blogPostCell",
            "selector": {
                "css": "div.slds-large-size_1-of-3.slds-medium-size_1-of-2.slds-size_1-of-1.slds-p-right_small.slds-m-bottom_small:nth-of-type(%d)",
                "args": [
                    {
                        "name": "index",
                        "type": "number"
                    }
                ]
            },
            "public": true
        },
        {
            "name": "blogListContainer",
            "type": [
                "actionable"
            ],
            "selector": {
                "css": "div.slds-grid.slds-wrap.slds-grid_vertical-stretch.resetMarginLeft.slds-grid_pull-padded-small.pull-padded-bottom-small"
            }
        }
    ]
}
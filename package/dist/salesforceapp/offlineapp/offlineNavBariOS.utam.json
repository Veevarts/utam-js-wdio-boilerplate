{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: **/XCUIElementTypeNavigationBar.",
            "Represents the iOS navigation bar for the Offline App.",
            "Click items, open the profile menu."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/offlineapp/offlineNavBar",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "ios_phone",
                "ios_tablet"
            ]
        }
    ],
    "selector": {
        "classchain": "**/XCUIElementTypeNavigationBar"
    },
    "elements": [
        {
            "name": "briefcaseSyncButton",
            "type": [
                "clickable"
            ],
            "nullable": true,
            "selector": {
                "accessid": "briefcase_sync"
            }
        },
        {
            "name": "createRecordButton",
            "type": [
                "clickable"
            ],
            "nullable": true,
            "selector": {
                "accessid": "Create Record"
            }
        },
        {
            "name": "button",
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`label CONTAINS '%s'`]",
                "args": [
                    {
                        "name": "label",
                        "type": "string"
                    }
                ]
            },
            "type": [
                "clickable"
            ]
        },
        {
            "name": "menuButton",
            "type": [
                "clickable"
            ],
            "selector": {
                "accessid": "userProfilePhoto"
            }
        }
    ],
    "methods": [
        {
            "name": "clickBriefcaseSync",
            "compose": [
                {
                    "element": "briefcaseSyncButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clickCreateRecord",
            "compose": [
                {
                    "element": "createRecordButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clickItem",
            "compose": [
                {
                    "element": "button",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "openProfileMenu",
            "compose": [
                {
                    "element": "menuButton",
                    "apply": "click"
                }
            ]
        }
    ]
}
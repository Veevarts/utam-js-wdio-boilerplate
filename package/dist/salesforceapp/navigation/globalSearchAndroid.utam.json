{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: className(\"androidx.drawerlayout.widget.DrawerLayout\").resourceId(\"com.salesforce.chatter:id/drawer_layout\").",
            "Represents the Android global search fragment.",
            "Start a global search or interact with the existing list of recent items."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/navigation/globalSearch",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "android_phone",
                "android_tablet"
            ]
        }
    ],
    "beforeLoad": [
        {
            "apply": "waitFor",
            "args": [
                {
                    "type": "function",
                    "predicate": [
                        {
                            "element": "root",
                            "apply": "isPresent"
                        }
                    ]
                }
            ]
        }
    ],
    "selector": {
        "uiautomator": "className(\"androidx.drawerlayout.widget.DrawerLayout\").resourceId(\"com.salesforce.chatter:id/drawer_layout\")"
    },
    "elements": [
        {
            "name": "searchBar",
            "type": "utam-salesforceapp/pageObjects/navigation/searchBar",
            "selector": {
                "uiautomator": "resourceId(\"com.salesforce.chatter:id/container\")"
            },
            "public": true
        },
        {
            "name": "searchResults",
            "type": "utam-salesforceapp/pageObjects/navigation/navItemsList",
            "public": true,
            "selector": {
                "uiautomator": "resourceId(\"com.salesforce.chatter:id/find_objects_view\")"
            }
        }
    ]
}
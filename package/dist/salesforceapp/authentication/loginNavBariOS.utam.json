{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: **/XCUIElementTypeNavigationBar[`name == 'Log In'`].",
            "Represents the navigation bar in the iOS login view controller.",
            "Load the screen to create or choose a different connection."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/authentication/loginNavBar",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "ios_phone",
                "ios_tablet"
            ]
        }
    ],
    "selector": {
        "classchain": "**/XCUIElementTypeNavigationBar[`name == 'Log In'`]"
    },
    "methods": [
        {
            "name": "chooseConnOption",
            "compose": [
                {
                    "element": "connBtn",
                    "apply": "click"
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "connBtn",
            "type": [
                "clickable"
            ],
            "selector": {
                "accessid": "choose connection button"
            }
        }
    ]
}
{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: resourceId(\"android:id/action_bar\").",
            "Represents the action bar in the Android EULA activity.",
            "Accept the terms."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/authentication/eula",
    "platform": "native",
    "profile": [
        {
            "platform": [
                "android_phone",
                "android_tablet"
            ]
        }
    ],
    "root": true,
    "selector": {
        "uiautomator": "resourceId(\"android:id/action_bar\")"
    },
    "methods": [
        {
            "name": "accept",
            "compose": [
                {
                    "element": "iaccept",
                    "apply": "click"
                }
            ]
        }
    ],
    "elements": [
        {
            "type": [
                "clickable"
            ],
            "name": "iaccept",
            "selector": {
                "uiautomator": "resourceIdMatches(\".*mi_accept\")"
            }
        }
    ]
}
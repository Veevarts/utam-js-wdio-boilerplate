{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: className(\"android.widget.FrameLayout\").className(\"android.widget.ListView\".",
            "Represents list view of login-related actions from the action bar in the Android login activity.",
            "Choose a login-related action: change server, clear cookies, or reload."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/authentication/loginNavBarOptions",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "android_phone",
                "android_tablet"
            ]
        }
    ],
    "selector": {
        "uiautomator": "className(\"android.widget.FrameLayout\").className(\"android.widget.ListView\")"
    },
    "methods": [
        {
            "name": "changeServer",
            "compose": [
                {
                    "element": "server",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clearCookies",
            "compose": [
                {
                    "element": "cookies",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "reload",
            "compose": [
                {
                    "element": "reload",
                    "apply": "click"
                }
            ]
        }
    ],
    "elements": [
        {
            "name": "server",
            "type": [
                "clickable"
            ],
            "selector": {
                "uiautomator": "resourceId(\"android:id/title\").text(\"Change Server\")"
            }
        },
        {
            "name": "cookies",
            "type": [
                "clickable"
            ],
            "selector": {
                "uiautomator": "resourceId(\"android:id/title\").text(\"Clear Cookies\")"
            }
        },
        {
            "name": "reload",
            "type": [
                "clickable"
            ],
            "selector": {
                "uiautomator": "resourceId(\"android:id/title\").text(\"Reload\")"
            }
        }
    ]
}
{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: **/XCUIElementTypeOther[$type = 'XCUIElementTypeNavigationBar' AND name == 'Mobile Security'$].",
            "Represents the navigation bar in the iOS 'Mobile Security' view controller.",
            "Refresh and resolve the security check and check the details of the result."
        ]
    },
    "implements": "utam-salesforceapp/pageObjects/authentication/mobileSecurity",
    "root": true,
    "platform": "native",
    "profile": [
        {
            "platform": [
                "ios_phone",
                "ios_tablet"
            ]
        }
    ],
    "selector": {
        "classchain": "**/XCUIElementTypeOther[$type = 'XCUIElementTypeNavigationBar' AND name == 'Mobile Security'$]"
    },
    "elements": [
        {
            "name": "infoAndHealthyPoliciesButton",
            "type": [
                "clickable"
            ],
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`name == 'Info and policy health button'`]"
            }
        },
        {
            "name": "recheckButton",
            "type": [
                "clickable"
            ],
            "selector": {
                "classchain": "**/XCUIElementTypeButton[`name == 'Recheck'`]"
            }
        }
    ],
    "methods": [
        {
            "name": "clickInfoAndHealthyPolicies",
            "compose": [
                {
                    "element": "infoAndHealthyPoliciesButton",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clickRefresh",
            "compose": [
                {
                    "element": "recheckButton",
                    "apply": "click"
                }
            ]
        }
    ]
}
{
    "description": {
        "author": "Salesforce",
        "text": [
            "Selector: div#main.",
            "Represents Oauth screen to allow access and manage data for the user.",
            "Choose 'Allow' or 'Deny' access and manage data for the user."
        ]
    },
    "root": true,
    "selector": {
        "css": "div#main"
    },
    "methods": [
        {
            "name": "clickAllow",
            "compose": [
                {
                    "element": "allow",
                    "apply": "scrollToTop"
                },
                {
                    "element": "allow",
                    "apply": "click"
                }
            ]
        },
        {
            "name": "clickDeny",
            "compose": [
                {
                    "element": "deny",
                    "apply": "scrollToTop"
                },
                {
                    "element": "deny",
                    "apply": "click"
                }
            ]
        }
    ],
    "elements": [
        {
            "type": [
                "clickable",
                "actionable"
            ],
            "name": "allow",
            "selector": {
                "css": "input#oaapprove"
            }
        },
        {
            "type": [
                "clickable",
                "actionable"
            ],
            "name": "deny",
            "selector": {
                "css": "input#oadeny"
            }
        }
    ]
}
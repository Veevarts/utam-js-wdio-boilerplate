{
    "name": "salesforce-pageobjects",
    "version": "6.0.0",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "license": "SEE LICENSE IN LICENSE",
    "type": "module",
    "exports": {
        "./package.json": "./package.json",
        "./ui-utam-pageobjects.config.json": "./dist/ui-utam-pageobjects.config.json",
        "./utam-communities-pageobjects.config.json": "./dist/utam-communities-pageobjects.config.json",
        "./utam-salesforceapp-pageobjects.config.json": "./dist/utam-salesforceapp-pageobjects.config.json",
        "./utam-sfsmobileapp-pageobjects.config.json": "./dist/utam-sfsmobileapp-pageobjects.config.json",
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./*": {
            "require": {
                "types": "./dist/*.d.cts",
                "default": "./dist/*.cjs"
            },
            "import": {
                "types": "./dist/*.d.ts",
                "default": "./dist/*.js"
            }
        }
    },
    "files": [
        "dist/index.js",
        "dist/index.cjs",
        "dist/**/*.utam.json",
        "dist/**/*.js",
        "dist/**/*.cjs",
        "dist/**/*.d.ts",
        "dist/**/*.d.cts",
        "dist/*.config.json"
    ],
    "scripts": {
        "clean": "rm -rf configs && rm -rf dist",
        "build": "./scripts/create-package.js",
        "build:copy-src": "./scripts/prebuild/copy-src-files.js --summary",
        "build:update-json-author": "./scripts/prebuild/update-json-author.js",
        "build:generate-configs": "./scripts/prebuild/generate-configs.js",
        "build:all-page-objects": "./scripts/build-page-objects.js",
        "build:core": "./scripts/build-page-objects.js --repos core",
        "build:lightning": "./scripts/build-page-objects.js --repos lightning",
        "build:helpers": "./scripts/build-page-objects.js --repos helpers",
        "build:mobile": "./scripts/build-page-objects.js --repos mobile",
        "build:merge-profiles": "./scripts/postbuild/merge-profiles.js",
        "build:update-extensions-imports": "./scripts/postbuild/update-extensions-imports.js",
        "build:remove-commonjs-cycle-deps": "./scripts/postbuild/fix-commonjs-import-cycles.js",
        "build:add-entry-points": "./scripts/postbuild/add-entry-points.js",
        "lint:unresolved-page-objects": "yarn eslint dist",
        "format": "prettier  --write 'scripts/**/*.{js,cjs,ts,json}'",
        "test": "yarn node --experimental-vm-modules $(yarn bin jest)"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "babel-plugin-tester": "^11.0.4",
        "es-module-lexer": "^1.4.1",
        "eslint": "^8.53.0",
        "eslint-plugin-import": "^2.28.1",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2",
        "jest": "^29.7.0",
        "magic-string": "^0.30.5",
        "pkg-dir": "^8.0.0",
        "prettier": "3.1.0",
        "resolve.exports": "^2.0.2",
        "utam": "^2.2.0"
    },
    "keywords": [
        "salesforce",
        "testing",
        "utam",
        "ui test automation model"
    ],
    "volta": {
        "node": "20.9.0",
        "yarn": "1.22.18"
    },
    "engines": {
        "node": ">= 18"
    }
}
